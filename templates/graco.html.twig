{% extends 'layout.html.twig' %}

{% block title %}{{parent()}}Zařízení Graco{% endblock %}
{% block description %}{{ parent() }}Používáme zařízení Graco pro nástřik pěnové izolace v obytných a komerčních budovách.{% endblock %}
{% block keywords %}{{ parent() }}Graco, graco, zařízení Graco, polyuretanové izolace, polyuretanové, izolace, izolace nástřikem,{% endblock %}

{% block body %}
    <section class="page-section bg-light" id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading text-uppercase">Používáme zařízení Graco<sup>&reg;</sup></h2>
                    <h3 class="section-subheading text-muted">Pěnové polyuretanové izolace nástřikem</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <ul class="timeline">
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <img class="rounded-circle img-fluid" src="{{ asset('img/graco-logo.jpg') }}" alt="">
                            </div>
                        </li>
                        <li>
                            <div class="timeline-image">
                                <img class="rounded-circle img-fluid" src="{{ asset('img/graco-1.jpg') }}" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4 class="subheading text-warning">Stříkací pistole pro aplikaci pěnové polyuretanové izolace nástřikem</h4>
                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <img class="rounded-circle img-fluid" src="{{ asset('img/graco-2.jpg') }}" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4 class="text-danger subheading">Zařízení pro nástřik pěnové izolace v obytných a komerčních budovách</h4>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="timeline-image">
                                <img class="rounded-circle img-fluid" src="{{ asset('img/graco-3.jpg') }}" alt="">
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4 class="text-success subheading">Dálkově ovládané pěnové a polymočovinové reaktory</h4>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
{% endblock %}
